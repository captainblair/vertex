{
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/|assets/|favicon.ico).*)",
            "destination": "/index.html"
        }
    ]
}